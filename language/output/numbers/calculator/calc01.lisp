; define functions

(defun add (x y)(+ x y))
(defun subtract (x y)(- x y))
(defun multiply (x y)(* x y))
(defun divide (x y)(/ x y))

; call functions

(print (add 1 2))      ; output: 3
(print (subtract 1 2)) ; output: -1
(print (multiply 1 2)) ; output: 2
(print (divide 1 2))   ; output: 1/2



